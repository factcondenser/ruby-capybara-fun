					break;
				default:
					advurl = ''+'%c'+'tel:'+ flashvars.clickToCallNumber+'';
					break;
			}
	}
	document.write('<a onmouseover="onHoverMaster();" target="'+vl_html_target+'" href="'+advurl+'"><img src="' + (isSSL == "true" ? dcgifSSL : dcgif) + '" border=0></a>');
}
